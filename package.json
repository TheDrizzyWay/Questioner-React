{
  "name": "questioner-react",
  "version": "1.0.0",
  "description": "questioner frontend react js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack.prod.js",
    "devstart": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest",
    "heroku-postbuild": "npm run build",
    "eslint-fix": "eslint --fix \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheDrizzyWay/Questioner-React.git"
  },
  "keywords": [
    "Questioner",
    "React"
  ],
  "author": "Christopher Iliyas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheDrizzyWay/Questioner-React/issues"
  },
  "homepage": "https://github.com/TheDrizzyWay/Questioner-React#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/enzymeConfig.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js",
      "\\.(css|scss)$": "<rootDir>/fileTransformer.js"
    }
  }
}
